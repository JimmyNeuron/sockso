
<%import>
    com.pugh.sockso.music.Track;
</%import>

<%args>
    Track track = null;
</%args>

{
    "id": <% track.getId() %>,
    "name": "<% track.getName() #j %>",

    "album": {
        "id": <% track.getAlbum().getId() %>,
        "name": "<% track.getAlbum().getName() #j %>"
    },

    "artist": {
        "id": <% track.getArtist().getId() %>,
        "name": "<% track.getArtist().getName() #j %>"
    }

}
